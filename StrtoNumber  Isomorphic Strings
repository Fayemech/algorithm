public:
    bool isIsomorphic(string s, string t) {
        if(replacestrtonumber(s)!=replacestrtonumber(t))    return false;
        return true;
    }
    string replacestrtonumber(string s){
        char a[128]={0};
        char tmp='0';
        for(int i=0;i<s.length();i++){
            char b=s.at(i);
                if(a[b]==0){
                    a[b]=tmp++;
                }
            s[i]=a[b];
        }
            return s;
    };
